{
	"name": "vsc-mad-white",
	"displayName": "vsc-mad-white",
	"publisher": "taichu-publisher",
	"description": "visual studio code extension for mad white",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloMadWhite"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloMadWhite",
				"title": "Hello MadWhite"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.helloMadWhite",
					"group": "YourGroup@1"
				}
			]
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.14.16",
		"@types/vscode": "^1.37.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.2.3",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
